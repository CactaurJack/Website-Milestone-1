<!DOCTYPE html>
<html>
<head>

    <title>@ViewData["Title"] - The Flying Saucer</title>

    <environment include="Development">
        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
        <link rel="stylesheet" href="~/css/site.css" />
    </environment>

</head>
<body>
	<nav class="navbar navbar-inverse navbar-fixed-top">
		<div class="container">
			<div class="navbar-header">
				<img src="~/images/logo.png" />
				<button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
					<span class="sr-only">Toggle navigation</span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
				</button>
			</div>
			<div class="navbar-collapse collapse">
				<ul class="nav navbar-nav">
					<li><a asp-page="/Index">Home</a></li>
					<li><a asp-page="/Privacy">Entrees</a></li>
					<li><a asp-page="/Privacy">Sides</a></li>
					<li><a asp-page="/Privacy">Drinks</a></li>
					<li><a asp-page="/Privacy">Privacy</a></li>
					<li><a asp-page="/About">About</a></li>
				</ul>
			</div>
		</div>
	</nav>
	<footer>
		<p>&copy; 2021 - The Flying Saucer LLC</p>
	</footer>

	<partial name="_CookieConsentPartial" />

	<div class="container body-content">
		@RenderBody()
		<hr />
		<footer>
			<p>&copy; 2021 - The Flying Saucer LLC</p>
		</footer>
	</div>




	@RenderSection("Scripts", required: false)
</body>
</html>
